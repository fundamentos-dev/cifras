---
layout: null
---
[
  {% assign files = site.static_files | where_exp: "item", "item.path contains '/examples/'" %}
  {% assign txt_files = "" | split: "" %}
  
  {% for file in files %}
    {% if file.extname == ".txt" %}
      {% assign txt_files = txt_files | push: file %}
    {% endif %}
  {% endfor %}

  {% for file in txt_files %}
    "{{ file.basename | replace: '.txt', '' }}"{% unless forloop.last %},{% endunless %}
  {% endfor %}
]